{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "Angular 2",
  "main": "index.js",
  "scripts": {
    "build": "WEBPACK_ENV=production webpack --progress --hide-modules",
    "start": "webpack-dev-server --history-api-fallback --content-base ./src --open",
    "e2e": "protractor",
    "postinstall": "npm run webdriver-update",
    "test": "karma start",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,tests,karma.entry.ts",
    "coverage:report": "istanbul report",
    "webdriver-update": "webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wkwiatek/angular2-webpack"
  },
  "author": "wkwiatek <wojtek.kwiatek@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/http": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@angular/router": "^3.0.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.2",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "codelyzer": "^2.0.0-beta.2",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "stylus": "^0.54.2",
    "stylus-loader": "^2.0.0",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^1.2.0",
    "tslint": "^4.1.1",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.0.0",
    "webpack": "^2.2.0-rc.1",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
